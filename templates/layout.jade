!!! 5
html
  head
    title #{page.title}
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width')
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400italic', rel='stylesheet', type='text/css')
    link(href='//netdna.bootstrapcdn.com/font-awesome/3.0.2/css/font-awesome.css', rel='stylesheet')
    script(src='/vendor/modernizr.js')
    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic|Anonymous+Pro:400,700,400italic,700italic|Merriweather:400,700,300')
    link(rel='stylesheet', href='/css/main.css')
    |  <!--Le HTML5 shim, for IE6-8 support of HTML5 elements--><!--if lt IE 9-->
    |  <!-- // <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script> -->

  body
    block content

    
    #modal-1.md-modal.md-effect-15
      .md-content
        form#contact-form(method="post", enctype="multipart/form-data", action="http://sfdevlabs.com:8080")

          input._honey(type='text', value="", data-val="honey")

          input(type='text', name="name", value="", placeholder='Your Name', data-val="required")

          input.val-email(type='email', name="email", value="", placeholder='Email', data-val="required")

          textarea(type='text', rows="5", name="body", placeholder='How can we help you?', data-val="required")

          div(data-val='notice')

          .button.pull-right.md-close(data-val="submit") Contact us

    #thanks-modal.md-modal.md-effect-15
      .md-content
        .text-center.large-text Thanks for your interest. We'll be in touch soon!
    .md-overlay
